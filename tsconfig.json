{
  "compilerOptions": {
    "target": "ES2017",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "react-jsx",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@style/*": [
        "./src/style/*"
      ],
      "@layouts/*": [
        "./src/layouts/*"
      ],
      "@containers/*": [
        "./src/containers/*"
      ],
      "@config/*": [
        "./src/config/*"
      ],
      "@components/*": [
        "./src/components/*"
      ],
      "@providers/*": [
        "./src/providers/*"
      ],
      "@hooks/*": [
        "./src/hooks/*"
      ],
      "@utils/*": [
        "./src/utils/*"
      ],
      "@context/*": [
        "./src/context/*"
      ],
      "@queries/*": [
        "./src/queries/*"
      ],
      "@store/*": [
        "./src/store/*"
      ],
      "@services/*": [
        "./src/services/*"
      ],
      "@mutations/*": [
        "./src/mutations/*"
      ],
      "@types/*": [
        "./src/utils/types/*"
      ],
      "@api/*": [
        "./src/app/api/*"
      ],
      "@data/*": [
        "./src/data/*"
      ]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    ".next/dev/types/**/*.ts"
, "tsconfig.seed.json"  ],
  "exclude": [
    "node_modules"
  ]
}
